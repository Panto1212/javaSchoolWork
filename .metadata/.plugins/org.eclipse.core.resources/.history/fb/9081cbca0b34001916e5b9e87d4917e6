package exercises;
import java.util.Scanner;

public class Courseunits {

	public static void main(String[] args) {
	Scanner scan = new Scanner(System.in);
	System.out.print("How many course units?");
	int n= scan.nextInt();
	
	for(int i=1; i<=n; i++) {
		System.out.println("Course "+i+" :");
	}

	}

}
