package variables;

public class Variables {

	public static void main(String[] args) {
		String firstName = "Patience", lastName;
		lastName= "Ankunda";
		System.out.println(firstName+lastName);
		int myAge = 21; 
		long phoneNumber;
		phoneNumber= 0781693730L;
		System.out.println(myAge\n phoneNumber);
		// TODO Auto-generated method stub

	}

}
